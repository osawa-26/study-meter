<% @title = "新しい投稿" %>
<h1><%= @title %></h1>
<%= render 'layouts/flash_messages' %>

<div id="generic-form">
  <%= form_with model: @record, url: [ :admin, @record ] do |f| %>
    <%= render "form", f: f %>
    <div class="buttons">
      <%= f.submit "投稿" %>
      <%= link_to "キャンセル", :admin_records %>
    </div>
  <% end %>
</div>
