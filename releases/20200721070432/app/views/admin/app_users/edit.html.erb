<% @title = "利用者様アカウント編集" %>
<h1><%= @title %></h1>

<div id="generic-form">
  <%= form_with model: @app_user, url: [ :admin, @app_user ] do |f| %>
    <%= render "form", f: f %>
    <div class="buttons">
      <%= f.submit "更新" %>
      <%= link_to "キャンセル", :admin_app_users %>
    </div>
  <% end %>
</div>
